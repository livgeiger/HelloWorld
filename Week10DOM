let parents= document.getElementById(tableDiv);


  document.getElementById('add-button').addEventListener( 'click', ()=>{
    let table = document.getElementById("list");
    var row = table.insertRow(1);
    row.insertCell(0).innerHTML= getElementById('nameEntered').value;
    row.insertCell(1).innerHTML= getElementById('itemEntered').value;
    row.insertCell(2).innerHTML= getElementById('numberEntered').value; 
    parents.appendChild(table);
    
  })




